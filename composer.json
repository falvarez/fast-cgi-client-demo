{
  "name": "hollodotme/fast-cgi-client-demo",
  "description": "This is a litte collection of example scripts to demonstrate the various usages of the FastCGI Client",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Holger Woltersdorf",
      "email": "hw@hollo.me"
    }
  ],
  "require": {
    "php": ">=7.1",
    "hollodotme/fast-cgi-client": "^2.5.0",
    "sensiolabs/ansi-to-html": "^1.1.3",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "hollodotme\\FastCGI\\ClientDemo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "hollodotme\\FastCGI\\ClientDemo\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "tm/tooly-composer-script": "^1.0"
  },
  "scripts": {
    "post-install-cmd": "Tooly\\ScriptHandler::installPharTools",
    "post-update-cmd": "Tooly\\ScriptHandler::installPharTools"
  },
  "extra": {
    "tools": {
      "phpunit": {
        "url": "https://phar.phpunit.de/phpunit-6.4.3.phar",
        "only-dev": true
      }
    }
  }
}
